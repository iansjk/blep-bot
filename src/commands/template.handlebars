          </th>
          <th><img
              src="data:image/png;base64,">
          </th>
          <th><img
              src="data:image/png;base64,">
          </th>
          <th><img
              src="data:image/png;base64,">
          </th>
          <th><img
              src="data:image/png;base64,">
          </th>
          <th><img
              src="data:image/png;base64,">
          </th>
          <th><img
              src="data:image/png;base64,">
          </th>
        </tr>
      </thead>
      <tbody>
        {{#each hitzoneData }}
        <tr>
          <td class="string-value">{{this.part}}</td>
          <td>{{ rawHzv this.sever this.monster.name }}</td>
          <td>{{ rawHzv this.blunt this.monster.name }}</td>
          <td>{{ rawHzv this.shot this.monster.name }}</td>
          <td>{{ eleHzv this.fire }}</td>
          <td>{{ eleHzv this.water }}</td>
          <td>{{ eleHzv this.thunder }}</td>
          <td>{{ eleHzv this.ice }}</td>
          <td>{{ eleHzv this.dragon }}</td>
          <td>{{ eleHzv this.stun }}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</body>

</html>